{
	"manifest_version" : 2,
	"name" : "Tabster",
	"version" : "1.0",	
	"description" : "This is an extension to make multitasking easier in chrome",
	"browser_action" :{
		"default_icon" : "images/icon.png",
		"default_popup" : "options.html"
	},
	"background":{
		"scripts": ["background.js"],
		"persistent": false
	},
	"permissions": [
		"tabs",
		"activeTab"
	]
}